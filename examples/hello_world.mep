contract HelloWorld {
    #[assume(returndatasize = 0x00)]
    block main {
        // copy the bytes into memory
        push(hello_world.size) push(hello_world.pc) push0 codecopy

        // return them
        push(hello_world.size) push0 return
    }

    block hello_world {
        // "Hello World!" as bytes
        0x48656c6c6f20576f726c6421
    }
}
